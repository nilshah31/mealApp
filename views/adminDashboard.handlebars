<div class='content container-fluid' style="background-color:#fafafa;">
  <div class="col-sm-12">
  {{#if success_msg}}
  <div class="alert alert-success">
    <span class="closebtn" onclick="this.parentElement.style.display='none';">&times;</span>
    {{success_msg}}
  </div>
  {{/if}}
  {{#if error_msg}}
    <div class="alert alert-danger">
      <span class="closebtn" onclick="this.parentElement.style.display='none';">&times;</span>
      {{error_msg}}
    </div>
  {{/if}}
  </div>

  <!--ProgressBar-->
  <div class="col-sm-4">
    <div id="work-in-progress" style="margin-left:-1em;margin-top:1.5em;">
      <div class="work-spinner"></div>
    </div>
    <script>
      $(window).bind("load", function () {
        $('#work-in-progress').fadeOut(100);
      });
      </script>
  </div>
  <!--ProgressBar-->

  <!-- ALl Partial Forms-->
  <div class="col-sm-12">
    {{> adminItemWiseOrders }}
  </div>
  <div class="col-sm-12">
    {{> adminOrders }}
  </div>
  <div class="col-sm-12">
    {{> adminLocation }}
  </div>
  <div class="col-sm-12">
    {{> adminItems }}
  </div>
  <div class="col-sm-12">
    {{> adminUsers }}
  </div>

  <div class="col-sm-12">
    <footer>
      <p>Made width <i class='fa fa-heart text-danger'></i> By Nil Shah</p>
    </footer>
  </div>
</div>

