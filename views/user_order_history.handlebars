<br />
<div class="container">
  <h1><i class="fa fa-history" aria-hidden="true"></i> ORDER HISTORY</h1>
  <div class="row">
    <div class="table-responsive">
      <table class="table table-bordered">
        <thead>
          <tr>
            <th>Order Date</th>
            <th>Order ID</th>
            <th>Item</th>
            <th>#</th>
            <th>Price</th>
            <th>Bill Amount</th>
            <th>Status</th>
          </tr>
        </thead>
        <tbody>
        {{#each object_item_hash}}
            <tr>
                <td>{{order_date}}</td>
                <td>12345</td>
                <td>{{order_itemName}}</td>
                <td>{{order_itemQty}}</td>
                <td>{{order_itemPrice}}</td>
                <td><i class="fa fa-inr"></i>&nbsp;{{total}}</td>
                <td><span class="label label-info">{{status}}</span></td>
            </tr>
          {{/each}}
          </tbody>
      </table>
    </div>
  </div>
</div>
